@model  PhotoContest.Web.Models.ViewModels.ContestViewModel
@{
    ViewBag.Title = "Contest Details";
}

<div class="row">

    <div class="row">
        <div class="col-md-3">
            @Model.Title
        </div>
        <div class="col-md-3">
            @Model.Description
        </div>
        <div class="col-md-3">
            @Model.OwnerName
        </div>
        <div class="col-md-3">
            @Model.IsClosed
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            @Model.DeadlineStrategy
        </div>
        <div class="col-md-3">
            @Model.VotingStrategy
        </div>
        <div class="col-md-3">
            @Model.RewardStrategy
        </div>
        <div class="col-md-3">
            @Model.ParticipationStrategy
        </div>
    </div>
    <hr />
</div>